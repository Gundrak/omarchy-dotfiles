{
    "window.commandCenter": true,
    "editor.fontSize": 16,
    "vim.leader": "<space>",
    "vim.insertModeKeyBindings": [
        {
            "before": ["j", "k"],
            "after": ["<Esc>"]
        }
    ],
    "vim.normalModeKeyBindings": [
        {
            "before": ["<leader>", "e"],
            "commands": ["workbench.view.explorer"]
        }
    ]
}
